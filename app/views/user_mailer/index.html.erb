
  <center> <h1> Dashboard Admin </h1>
  <br/>
  <br/>
  <h3> Users </h3>
  <p>Number of user : <%= User.all.length %>  </p>
  <ul>
  <% User.all.each do |user| %>
  <li><%= user.email %></li>
  <%= link_to 'See user', profile_path(user.id) %>
  <% end %>
  </ul>
  <h3> Articles </h3>
  <p>Number of article create : <%= Article.all.length %> </p>
  <% Article.all.each do |article| %>
  <div>
<p>  <%= article.title %></p>
  <%= link_to "See article", article_path(article.id) %>
  <%= link_to "Delete article", article_path(article.id), :method => :delete %>
  </div>
  <% end %>
  <br/>
  <h2> Article report </h2>
    </center>
